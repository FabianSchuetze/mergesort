@techreport{adam_stock_2013,
	type = {Working {Paper}},
	title = {Stock {Market} {Volatility} and {Learning}},
	url = {https://ideas.repec.org/p/bge/wpaper/336.html},
	abstract = {Consumption based asset pricing models with time-separable preferences can generate realistic amounts of stock price volatility if one allows for small deviations from rational expectations. We consider rational investors who entertain subjective prior beliefs about price behavior that are not equal but close to rational expectations. Optimal behavior then dictates that investors learn about price behavior from past price observations. We show that this imparts momentum and mean reversion into the equilibrium behavior of the price dividend ratio, similar to what can be observed in the data. When estimating the model on U.S. stock price data using the method of simulated moments, it can quantitatively account for the observed stock price volatility, the persistence of the price-dividend ratio, and the predictability of long-horizon returns. For reasonable degrees of risk aversion, the model generates up to one half of the equity premium observed in the data. It also passes a formal statistical test for the overall goodness of fit, provided one excludes the equity premium from the set of moments to be matched.},
	number = {336},
	urldate = {2014-10-11},
	institution = {Barcelona Graduate School of Economics},
	author = {Adam, Klaus and Marcet, Albert and Nicolini, Juan Pablo},
	year = {2013},
	keywords = {Learning, Asset pricing, Internal rationality, subjective beliefs},
	file = {RePEc PDF:/home/fabian/Zotero/storage/K2IV448N/Adam et al. - 2013 - Stock Market Volatility and Learning.pdf:application/pdf;RePEc Snapshot:/home/fabian/Zotero/storage/G9GH9B7U/336.html:text/html}
}

@article{adam_internal_2011,
	series = {Incompleteness and {Uncertainty} in {Economics}},
	title = {Internal rationality, imperfect market knowledge and asset prices},
	volume = {146},
	issn = {0022-0531},
	url = {http://www.sciencedirect.com/science/article/pii/S0022053110001638},
	doi = {10.1016/j.jet.2010.11.003},
	abstract = {We present a decision theoretic framework in which agents are learning about market behavior and that provides microfoundations for models of adaptive learning. Agents are ‚Äòinternally rational‚Äô, i.e., maximize discounted expected utility under uncertainty given dynamically consistent subjective beliefs about the future, but agents may not be ‚Äòexternally rational‚Äô, i.e., may not know the true stochastic process for payoff relevant variables beyond their control. This includes future market outcomes and fundamentals. We apply this approach to a simple asset pricing model and show that the equilibrium stock price is then determined by investors º expectations of the price and dividend in the next period, rather than by expectations of the discounted sum of dividends. As a result, learning about price behavior affects market outcomes, while learning about the discounted sum of dividends is irrelevant for equilibrium prices. Stock prices equal the discounted sum of dividends only after making very strong assumptions about agents º market knowledge.},
	number = {3},
	urldate = {2014-10-10},
	journal = {Journal of Economic Theory},
	author = {Adam, Klaus and Marcet, Albert},
	month = may,
	year = {2011},
	keywords = {Learning, Internal rationality, Consumption based asset pricing},
	pages = {1224--1252},
	file = {ScienceDirect Full Text PDF:/home/fabian/Zotero/storage/AU5QNZI3/Adam and Marcet - 2011 - Internal rationality, imperfect market knowledge a.pdf:application/pdf;ScienceDirect Snapshot:/home/fabian/Zotero/storage/XP9ZPME8/Adam and Marcet - 2011 - Internal rationality, imperfect market knowledge a.html:text/html}
}

