	note = {arXiv: 1206.2944},
	keywords = {Computer Science - Machine Learning, Statistics - Machine Learning},
	file = {arXiv Fulltext PDF:/home/fabian/Zotero/storage/34X98VFX/Snoek et al. - 2012 - Practical Bayesian Optimization of Machine Learnin.pdf:application/pdf;arXiv.org Snapshot:/home/fabian/Zotero/storage/2ZQG3SEB/1206.html:text/html}
