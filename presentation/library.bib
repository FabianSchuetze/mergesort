	institution = {National Bureau of Economic Research},
	author = {Fuster, Andreas and Hebert, Benjamin and Laibson, David},
	month = aug,
	year = {2011},
	file = {NBER Full Text PDF:/home/fabian/Zotero/storage/MMB65GAM/Fuster et al. - 2011 - Natural Expectations, Macroeconomic Dynamics, and .pdf:application/pdf}
}

@article{ge_escaping_2015,
	title = {Escaping {From} {Saddle} {Points} --- {Online} {Stochastic} {Gradient} for {Tensor} {Decomposition}},
